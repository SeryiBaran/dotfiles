source $(dirname $(gem which colorls))/tab_complete.sh

if [ $TILIX_ID ] || [ $VTE_VERSION ]; then
        source /etc/profile.d/vte-2.91.sh
fi

export EDITOR="micro"

if [ -f ~/.aliases ]; then
    . ~/.aliases
fi

if [ -f ~/extract.sh ]; then
    . ~/extract.sh
fi

export VOLTA_HOME="$HOME/.volta"
export PATH="$VOLTA_HOME/bin:$PATH"
